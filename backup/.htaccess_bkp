<IfModule mod_rewrite.c>

   Options +FollowSymLinks
  
  RewriteEngine on
   RewriteCond %{HTTP_HOST} ^smsstriker.com
   RewriteCond %{HTTP_HOST} ^www.smsstriker.com
   RewriteRule ^([a-z0-9_-]+)\.html$ index.php/page/$1 [L]
   RewriteCond %{REQUEST_FILENAME} !-f
   RewriteCond %{REQUEST_FILENAME} !-d
   RewriteCond $1 !^(index\.php|asset|robots\.txt)

RewriteRule ^(.*)$ ./index.php/$1 [L,QSA]
RewriteCond %{THE_REQUEST} \ (.*)//+(.*)\  [NC]
RewriteRule .* %1/%2 [R=301,L]
RewriteRule ^index\.html$ / [R=301,L]
RewriteRule ^(.*)/index\.html$ /$1/ [R=301,L]
Redirect 301 /index.php/index_new /
Redirect 301 /newpages /
Redirect 301 /index_new /error.php
Redirect 301 /privacy-statement /
#Redirect 301 /index.php/index /index.php/index_new
Redirect 301 /old/index.php/index /index.php/index_new



RewriteRule ^newpages(/)?$ /error.php [R]
RewriteRule ^old(/)?$ /index.php/index_new [R]
Redirect 301 /old/index.php/index_new /index.php/index_new

Redirect 301 /old/index.php/index_new/login.html /index.php/index_new
ErrorDocument 404 /error.php
AddOutputFilterByType DEFLATE text/html text/xml text/css text/javascript

#RewriteCond %{HTTP_HOST} ^smsstriker.com$ [NC]
#RewriteRule ^(.*)$ http://www.smsstriker.com/$1 [L,R=301]

#RewriteCond %{HTTP_HOST} ^www.strikersoft.in$ [NC]
#RewriteRule ^(.*)$ http://www.smsstriker.com/$1 [L,R=301]

#RewriteCond %{HTTP_HOST} ^strikersoft.in$ [NC]
#RewriteRule ^(.*)$ http://www.smsstriker.com/$1 [L,R=301]

<FilesMatch "\.(jpg|jpeg|png|gif|swf|css|js)$">
    Header set Cache-Control "max-age=604800, public"
</FilesMatch>

</IfModule>

#Gzip
<ifmodule mod_deflate.c>
AddOutputFilterByType DEFLATE text/text text/html text/plain text/xml text/css application/x-javascript application/javascript text/javascript
</ifmodule>
#End Gzip
